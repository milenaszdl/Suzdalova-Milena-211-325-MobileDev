<Window x:Class="wpfPageViewer.ViewPage"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:wpfPageViewer"
        mc:Ignorable="d"
        Title="Редактировать страницу" Height="450" Width="800">
    <Grid>
        <StackPanel x:Name="ListPages">
            <ListBox Margin="10" x:Name="SelectPage">
                <ListBox.ItemTemplate>
                    <DataTemplate>
                        <TextBlock Text="{Binding}"/>
                    </DataTemplate>
                </ListBox.ItemTemplate>
            </ListBox>
        </StackPanel>
        <StackPanel x:Name="PlaceForCanvas"></StackPanel>
    </Grid>
</Window>
